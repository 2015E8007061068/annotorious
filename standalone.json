{
  "id": "annotorious",
  "define": {
    "goog.DEBUG": false,
    "goog.dom.ASSUME_STANDARDS_MODE": true
  },
  "inputs": [
    "src/anno.js",
    "src/plugin.js" // to set up the plugin namespace
  ],
  "paths": [
    "src",
    "templates"
  ],
  "externs": [
    "externs/api.js",
    "externs/openlayers.js",
    "//json.js",
    "//webkit_console.js"
  ],
  "mode": "ADVANCED", // "RAW" or "ADVANCED"
  "level": "DEFAULT", // "DEFAULT" or "VERBOSE"
  "pretty-print": false,
  "debug": false
}
